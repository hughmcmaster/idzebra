SHELL=/bin/sh

CC=@CC@
CPP=@CPP@
RANLIB=@RANLIB@
CDEFS=@DEFS@ @ODEFS@
LIBS=@LIBS@

YAZLIB=@YAZLIB@
YAZINC=@YAZINC@

INCLUDE=-I../include $(YAZINC)
DEFS=$(CDEFS) $(INCLUDE)
LIB=../lib/rset.a
PROG=
PO=rset.o rstemp.o rsisam.o rsnull.o rsbool.o rsisamc.o rsm_or.o rsisams.o

all: $(LIB)

$(LIB): $(PO)
	rm -f $(LIB)
	ar qc $(LIB) $(PO)
	$(RANLIB) $(LIB)

.c.o:
	$(CC) -c $(DEFS) $(CFLAGS) $<

clean:
	rm -f *.[oa] core mon.out gmon.out errlist

depend:
	sed '/^#Depend/q' <Makefile >Makefile.tmp
	$(CPP) $(DEFS) -M *.c >>Makefile.tmp
	mv -f Makefile.tmp Makefile

